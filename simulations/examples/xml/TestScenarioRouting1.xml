<filteringDatabases>
	<filteringDatabase id="switchA">
	    <static>
	        <forward>
	        	<!-- Forward packets addressed to HeadUnit to switchB -->
	        	<individualAddress macAddress="00-00-00-00-00-09" port="0" />
	        	<!-- Forward packets addressed to AV_sink to AV_sink -->
	        	<individualAddress macAddress="00-00-00-00-00-01" port="1" />
	        </forward>
	    </static>
	</filteringDatabase>
	<filteringDatabase id="switchB">
	    <static>
	        <forward>
	        	<!-- Forward packets addressed to HeadUnit to HeadUnit -->
	        	<individualAddress macAddress="00-00-00-00-00-09" port="7" />
	        	<!-- Forward packets addressed to PUCAM to PUCAM -->
	        	<individualAddress macAddress="00-00-00-00-00-08" port="6" />
	        </forward>
	    </static>
	</filteringDatabase>
</filteringDatabases>